{
  "name": "roxy-music",
  "version": "1.0.0",
  "description": "client-server music player and sync service",
  "main": "index.js",
  "scripts": {
    "start": "node bundles/server.js",
    "debug": "node-debug -p 8888 bundles/server.js",
    "lint": "eslint --ext .jsx,.js . || exit 0",
    "test": "node scripts/tests.js",
    "coverage": "node scripts/tests.js --coverage",
    "build": "webpack -d --config webpack.config/development.js",
    "watch": "webpack -d --config webpack.config/development.js --watch &",
    "production": "webpack -p --config webpack.config/production.js",
    "electron": "electron bundles/electron.js",
    "updateModules": "updtr",
    "qq": "audio-metadata -t id3v2 -c 100 -q 300 \"../Music/vangelis - heaven and hell.mp3\""
  },
  "author": "Daniel Barreiro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satyam/RoxyMusic.git"
  },
  "bugs": {
    "url": "https://github.com/Satyam/RoxyMusic/issues"
  },
  "homepage": "https://github.com/Satyam/RoxyMusic#readme",
  "myWebServer": {
    "port": 8080,
    "host": "http://localhost",
    "restAPIpath": "/data/v2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "denodeify": "^1.2.1",
    "electron-prebuilt": "^1.3.5",
    "express": "^4.14.0",
    "fluent-ffmpeg": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.15.0",
    "react": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-audio-player": "^0.2.7",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.0",
    "react-router-redux": "^4.0.5",
    "recompose": "^0.20.2",
    "recursive-readdir": "^2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sqlite3": "^3.1.5"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "commander": "^2.9.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "react-addons-perf": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "redux-devtools": "^3.3.1",
    "source-map-support": "^0.4.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2"
  }
}
