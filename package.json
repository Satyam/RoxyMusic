{
  "name": "roxy-music",
  "version": "1.0.0",
  "description": "client-server music player and sync service",
  "main": "index.js",
  "scripts": {
    "start": "node bundles/webServer.js",
    "debug": "node-debug -p 8888 bundles/webServer.js",
    "lint": "eslint --ext .jsx,.js . || exit 0",
    "test": "jest --config jest/config.json",
    "build": "webpack -d --config webpack.config/development.js",
    "watch": "webpack -d --config webpack.config/development.js --watch &",
    "production": "webpack -p --config webpack.config/production.js",
    "electron": "electron bundles/electronServer.js",
    "updateModules": "updtr --test-stdout",
    "rebuildElectron": "electron-rebuild",
    "precordova": "cp bundles/cordova.js cordova/www/index.js && cp bundles/cordova.css cordova/www/index.css && cp bundles/cordova.*.map cordova/www && cp server/data.sql cordova/www/res && cp cordova/index.html cordova/www",
    "cordova": "cd cordova && cordova build"
  },
  "author": "Daniel Barreiro",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Satyam/RoxyMusic.git"
  },
  "bugs": {
    "url": "https://github.com/Satyam/RoxyMusic/issues"
  },
  "homepage": "https://github.com/Satyam/RoxyMusic#readme",
  "myWebServer": {
    "port": 8080,
    "host": "http://localhost",
    "restAPIpath": "/data/v2"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "body-parser": "^1.16.1",
    "bootstrap": "^3.3.6",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.1",
    "denodeify": "^1.2.1",
    "diff": "^3.2.0",
    "electron-prebuilt": "^1.4.13",
    "express": "^4.14.1",
    "fluent-ffmpeg": "^2.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "mp3-duration": "^1.0.10",
    "musicmetadata": "^2.0.5",
    "path-to-regexp": "^1.7.0",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-sortable-hoc": "^0.5.0",
    "recompose": "^0.22.0",
    "recursive-readdir": "^2.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^3.1.8",
    "uuid": "^3.0.1",
    "websql": "^0.4.4"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "bluebird": "^3.4.7",
    "commander": "^2.9.0",
    "css-loader": "^0.26.1",
    "electron-devtools-installer": "^2.1.0",
    "electron-rebuild": "^1.5.7",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "has": "^1.0.1",
    "jest": "^18.1.0",
    "jest-css-modules": "^1.1.0",
    "jest-fetch-mock": "^1.0.7",
    "nock": "^9.0.5",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "redux-devtools": "^3.3.2",
    "redux-mock-store": "^1.2.2",
    "source-map-support": "^0.4.11",
    "style-loader": "^0.13.1",
    "updtr": "^0.2.3",
    "webpack": "^1.14.0"
  }
}
